{
  "name": "tilda",
  "version": "1.0.0",
  "description": "A service for mocking services",
  "main": "dist/index.js",
  "scripts": {
    "dev": "SEEDS_DIR=./seeds SEED=./seed.json nodemon ./src/index.ts",
    "lint": "eslint src/*/**.ts test/**/*.ts --fix",
    "prebuild": "npm run lint",
    "build": "tsc",
    "build:prod": "tsc",
    "prestart": "npm run build",
    "start": "node .",
    "test": "jest --watch --coverage",
    "test:cicd": "jest --coverage --runInBand --detectOpenHandles --forceExit"
  },
  "author": "Zac Zacal",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.199",
    "@types/node": "^20.8.0",
    "@types/supertest": "^2.0.13",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  }
}

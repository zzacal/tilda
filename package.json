{
  "name": "tilda",
  "version": "1.0.0",
  "description": "A service for mocking services",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src/*/**.ts test/**/*.ts --fix",
    "prebuild": "npm run lint",
    "build": "tsc",
    "build:prod": "tsc",
    "prestart": "npm run build",
    "start": "node .",
    "test": "jest --watch --coverage",
    "test:cicd": "jest --coverage --runInBand --detectOpenHandles --forceExit"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.3",
    "lodash": "^4.17.21",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.25",
    "@types/supertest": "^2.0.12",
    "eslint": "^8.13.0",
    "jest": "^27.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0"
  }
}
